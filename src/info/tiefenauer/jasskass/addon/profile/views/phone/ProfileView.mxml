<?xml version="1.0" encoding="utf-8"?>
<views:MobileView xmlns:fx="http://ns.adobe.com/mxml/2009" 
				  xmlns:s="library://ns.adobe.com/flex/spark" 
				  xmlns:views="info.tiefenauer.jasskass.app.views.*"
				  implements="info.tiefenauer.jasskass.addon.profile.views.interfaces.IProfileView"
				  title="My Profile"
				  >
	
	<fx:Script>
		<![CDATA[
			import info.tiefenauer.jasskass.addon.profile.model.interfaces.IProfile;
			public function get profile():IProfile{
				if (data is IProfile)
					return data as IProfile;
				return null;
			}
			public function set profile(value:IProfile):void{
				data = value;
			}
		]]>
	</fx:Script>
</views:MobileView>
